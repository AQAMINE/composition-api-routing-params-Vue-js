"products","selectedProduct","value","find","product","id","pid","title","price","description"],"sourceRoot":"","sources":["/Users/aqamine/Documents/Projects/composition api routing , params Vue js/src/pages/ProductDetails.vue"],"sourcesContent":["<template>\n  <section>\n    <h2>{{ title }}</h2>\n    <h3>${{ price}}</h3>\n    <p>{{ description}}</p>\n  </section>\n</template>\n\n<script>\nimport { inject } from 'vue';\n\nexport default {\n  props: ['pid'],\n  setup(props) {\n    const products = inject('products');\n\n\n    //find selected product pid from route\n    const selectedProduct = products.value.find(product => product.id === props.pid);\n\n    const title = selectedProduct.title;\n    const price = selectedProduct.price;\n    const description = selectedProduct.description;\n\n    return { title, price, description };\n  },\n};\n</script>\n\n\n<style scoped>\nsection {\n  margin: 3rem auto;\n  max-width: 40rem;\n  padding: 1rem;\n  border-radius: 12px;\n  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.26);\n}\n</style>"]}]}